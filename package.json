{
	"name": "@cldmv/holdmytask",
	"version": "1.6.0",
	"description": "A tiny task queue that waits until your task is ready",
	"main": "./index.cjs",
	"module": "./index.mjs",
	"types": "./types/index.d.mts",
	"exports": {
		".": {
			"types": "./types/index.d.mts",
			"import": "./index.mjs",
			"require": "./index.cjs"
		},
		"./devcheck": {
			"types": "./types/devcheck.d.mts",
			"import": "./devcheck.mjs"
		},
		"./main": {
			"development": {
				"types": "./types/src/hold-my-task.d.mts",
				"import": "./src/hold-my-task.mjs"
			},
			"types": "./types/dist/hold-my-task.d.mts",
			"import": "./dist/hold-my-task.mjs"
		}
	},
	"engines": {
		"node": ">=18.12"
	},
	"scripts": {
		"test": "vitest run --config .configs/vitest.config.mjs",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:types": "tsc --noEmit --project .configs/tsconfig.dts.jsonc",
		"lint": "eslint --config .configs/eslint.config.mjs .",
		"build": "node build.mjs",
		"build:types": "tsc --project .configs/tsconfig.dts.jsonc",
		"build:ci": "npm run build:types && npm run test:types && npm run build",
		"precommit": "npm run build:types && npm run test:types && npm run lint && npm run test"
	},
	"keywords": [
		"task",
		"queue",
		"scheduler",
		"priority",
		"delay",
		"concurrency",
		"async",
		"promise",
		"callback",
		"timeout",
		"expire",
		"bypass",
		"task-queue",
		"job-queue",
		"priority-queue",
		"nodejs",
		"esm",
		"cjs",
		"typescript"
	],
	"author": {
		"name": "Shinrai",
		"company": "CLDMV",
		"email": "git+npm@cldmv.net",
		"url": "https://cldmv.net"
	},
	"contributors": [
		{
			"name": "CLDMV",
			"url": "https://github.com/CLDMV"
		}
	],
	"license": "Apache-2.0",
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/shinrai"
	},
	"type": "module",
	"devDependencies": {
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.39.1",
		"@eslint/json": "^0.14.0",
		"@eslint/markdown": "^7.5.1",
		"@html-eslint/eslint-plugin": "^0.48.0",
		"@html-eslint/parser": "^0.48.0",
		"@types/node": "^20.0.0",
		"@vitest/ui": "^4.0.8",
		"eslint": "^9.39.1",
		"globals": "^16.5.0",
		"prettier": "^3.0.0",
		"typescript": "^5.0.0",
		"vitest": "^4.0.8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CLDMV/holdmytask.git"
	},
	"bugs": {
		"url": "https://github.com/CLDMV/holdmytask/issues"
	},
	"homepage": "https://github.com/CLDMV/holdmytask#readme",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.mjs",
		"index.cjs",
		"README.md",
		"LICENSE",
		"types/dist/",
		"types/index.d.mts",
		"types/index.d.mts.map",
		"dist/"
	],
	"sideEffects": false
}
